<configure
    xmlns="http://namespaces.zope.org/zope"
    xmlns:browser="http://namespaces.zope.org/browser">
    
    <browser:page
             for="*"
             name="transmex"
             template="transmex.pt"
             permission="zope2.View"
             />

    <browser:page
             for="*"
             name="transmin"
             template="transmin.pt"
             permission="zope2.View"
             />

    <browser:page
             for="*"
             name="transmogrify"
             template="transmogrify.pt"
             permission="zope2.View"
             />

</configure>
