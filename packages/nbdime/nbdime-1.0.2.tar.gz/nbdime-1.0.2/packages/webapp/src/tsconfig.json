  "compilerOptions": {
    "target": "es6",
    "lib": [
      "dom",
      "es6"
    ],
    "module": "commonjs",
    "moduleResolution": "node",
    "sourceMap": true,
    "outDir": "../build",
    "noImplicitAny": false,
    "noEmitOnError": true,
    "skipLibCheck": true,
    "strictNullChecks": true
  }
}
