{
    "name": "lightfm",
    "class": "mlblocks.primitives.custom.predictors.lightfm.LightFM",
    "fit": "fit",
    "produce": "predict",
    "fixed_hyperparameters": {
        "epochs": 1,
        "num_threads": 1
    },
    "tunable_hyperparameters": {
        "loss": {
            "type": "string",
            "values": ["warp", "logistic", "bpr", "warp-kos"]
        },
        "learning_schedule": {
            "type": "string",
            "values": ["adagrad", "adadelta"]
        },
        "no_components": {
            "type": "int",
            "values": [5, 15]
        },
        "k": {
            "type": "int",
            "values": [2, 10]
        }
    },
    "root_hyperparameters": [],
    "conditional_hyperparameters": {}
}
