{
    "name": "tfidf_transformer",
    "class": "sklearn.feature_extraction.text.TfidfTransformer",
    "fit": "fit",
    "produce": "transform",
    "fixed_hyperparameters": {},
    "tunable_hyperparameters": {
        "norm": {
            "type": "string",
            "values": ["l1", "l2"]
        },
        "use_idf": {
            "type": "bool",
            "values": [true, false]
        },
        "smooth_idf": {
            "type": "bool",
            "values": [true]
        },
        "sublinear_tf": {
            "type": "bool",
            "values": [false]
        }
    },
    "root_hyperparameters": ["norm", "use_idf"],
    "conditional_hyperparameters": {}
}
